{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "resolveJsonModule": true,
    "paths": {
      "react": [ "./node_modules/@types/react" ]
      }
  },
  "include": [
    "src",
  ],
  "exclude": [
    "src/**.js"
  ],

}
